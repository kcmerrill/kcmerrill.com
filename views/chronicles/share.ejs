<html itemscope itemtype="http://schema.org/Article">
<head>
<!-- Place this data between the <head> tags of your website -->
<title><%= chronicle.title.text %></title>
<meta name="description" content="Adventurer. Dreamer. Explorer. Doer. Full time Software engineer, part time running/soccer/outdoor/camping loon." />

<!-- Google Authorship and Publisher Markup -->
<link rel="author" href="https://plus.google.com/+kcMerrill/posts"/>
<link rel="publisher" href=â€https://plus.google.com/+kcMerrill"/>

<!-- Schema.org markup for Google+ -->
<meta itemprop="name" content="<%= chronicle.title.text %>">
<meta itemprop="description" content="<%= chronicle.content.text %>">
<meta itemprop="image" content="<%= chronicle.media %>">

<!-- Twitter Card data -->
<meta name="twitter:card" content="<%= chronicle.media %>">
<meta name="twitter:site" content="@themayor">
<meta name="twitter:title" content="<%= chronicle.title.text %>">
<meta name="twitter:description" content="<%= chronicle.content.text %>">
<meta name="twitter:creator" content="@themayor">
<!-- Twitter summary card with large image must be at least 280x150px -->
<meta name="twitter:image:src" content="<%= chronicle.media %>">

<!-- Open Graph data -->
<meta property="og:title" content="<%= chronicle.title.text %>" />
<meta property="og:type" content="article" />
<meta property="og:url" content="http://kcmerrill.com/#/<%= chronicle.id %>" />
<meta property="og:image" content="<%= chronicle.media %>" />
<meta property="og:video" content="<%= chronicle.media %>" />
<meta property="og:description" content="<%= chronicle.content.text %>" />
<meta property="og:site_name" content="The chronicles of kc" />
</head>
<body>
</body>
</html>
